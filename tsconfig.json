{
  "compilerOptions": {
    "target": "ES2019",
    "module": "ESNext",
    "moduleResolution": "node",
    "lib": ["ES2019", "DOM", "DOM.Iterable"],
    "incremental": true,
    "noEmit": true,
    "tsBuildInfoFile": "tsconfig.tsbuildinfo",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "importsNotUsedAsValues": "error",
    "isolatedModules": true
  },
  "include": ["src/", "test/"]
}
